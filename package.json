{
  "name": "joplin-to-obsidian",
  "version": "1.0.0",
  "description": "从 Joplin 导入笔记到 Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "deploy": "npm run build && node deploy.mjs",
    "release": "node release.mjs"
  },
  "keywords": [
    "obsidian",
    "joplin",
    "import",
    "migration"
  ],
  "author": "lizhifeng",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "sql.js": "^1.8.0"
  }
}
